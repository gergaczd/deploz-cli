{
  "name": "deploz-cli",
  "version": "0.0.2",
  "author": "gergaczd @gergaczd",
  "bin": {
    "deploz": "./bin/run"
  },
  "bugs": "https://github.com/gergaczd/deploz-cli/issues",
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/plugin-autocomplete": "0.2.0",
    "@oclif/plugin-help": "3.2.0",
    "@oclif/plugin-not-found": "1.2.4",
    "@oclif/plugin-update": "^1.3.10",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "conf": "7.1.2",
    "prompts": "2.3.2",
    "simple-git": "2.19.0",
    "tslib": "1.13.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@oclif/test": "1.2.6",
    "@types/chai": "4.2.12",
    "@types/mocha": "5.2.7",
    "@types/node": "10.17.28",
    "@types/prompts": "2.0.8",
    "chai": "4.2.0",
    "eslint": "5.16.0",
    "eslint-config-oclif": "3.1.0",
    "eslint-config-oclif-typescript": "0.1.0",
    "globby": "10.0.2",
    "mocha": "^5.2.0",
    "nock": "13.0.4",
    "nyc": "14.1.1",
    "ts-node": "8.6.2",
    "typescript": "3.9.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/gergaczd/deploz-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "deploz",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 7
    }
  },
  "repository": "gergaczd/deploz-cli",
  "scripts": {
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "pack": "oclif-dev pack",
    "postpack": "rm -f oclif.manifest.json",
    "test": "nyc --extension .ts mocha --forbid-only \"**/*.test.ts\"",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "test:unit": "mocha \"**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
